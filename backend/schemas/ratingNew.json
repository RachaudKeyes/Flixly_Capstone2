{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/jobNew.schema.json",
  "type": "object",
  "properties": {
    "userId": {
      "type": "integer",
      "minimum": 0
    },
    "movieId": {
      "type": "integer",
      "minimum": 0
    },
    "rating": {
      "type": "number",
      "minimum": 0,
      "maximum": 5
    }
  },
  "additionalProperties": false,
  "required": [
    "userId",
    "movieId",
    "rating"
  ]
}
